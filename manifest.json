{
    "manifest_version": 3,
    "name": "Country Data Dashboard",
    "version": "1.0",
    "description": "A Chrome extension to display key data about top countries.",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png"
        }
    },
    "host_permissions": [
        "http://127.0.0.1:5000/"
    ]
}